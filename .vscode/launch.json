    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Android Development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": ["--flavor", "development"]
        },
        {
            "name": "Web Development",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": ["-d", "chrome", "--web-port", "8686"]
        },
        {
            "name": "Android Production",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_production.dart",
            "args": ["--flavor", "production"]
        },
        {
            "name": "Web Production",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_production.dart",
            "args": ["-d", "chrome"]
        }
    ]
}